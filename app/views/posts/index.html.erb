<h1>FakeGram</h1>
<%= link_to "New Post", new_post_path %>
<%= button_to "Log Out", destroy_user_session_path, method: :delete %>
<ul>
  <% @posts.each do |post| %>
    <li>
      <%= post.user.username %>
    </li>
    <li>
      <%= image_tag post.pic.variant(:gravity=>"Center", resize: "400x400^", crop: '400x400+0+0') %>
    </li>
    <li>
      <%= post.text %>
    </li>
  <% end %>
</ul>
